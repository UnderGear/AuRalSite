<h1>Listing synthinstances</h1>

<table>
  <tr>
    <th>Params</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @synthinstances.each do |synthinstance| %>
  <tr>
    <td><%= synthinstance.params %></td>
    <td><%= link_to 'Show', synthinstance %></td>
    <td><%= link_to 'Edit', edit_synthinstance_path(synthinstance) %></td>
    <td><%= link_to 'Destroy', synthinstance, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

