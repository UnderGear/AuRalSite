<h1>Listing userparams</h1>

<table>
  <tr>
    <th>User</th>
    <th>Value_1</th>
    <th>Value_2</th>
    <th>Value_3</th>
    <th></th>
    <th></th>
  </tr>

<% @userparams.each do |userparam| %>
  <tr>
    <td><%= userparam.user_id %></td>
    <td><%= userparam.value_1 %></td>
    <td><%= userparam.value_2 %></td>
    <td><%= userparam.value_3 %></td>
    <td><%= link_to 'Show', userparam %></td>
    <td><%= link_to 'Edit', edit_userparam_path(userparam) %></td>
    <td><%= link_to 'Destroy', userparam, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Userparam', new_userparam_path %>
